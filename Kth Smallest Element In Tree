class Solution:
	def kthsmallest(self, A, B):
        stack = []
        curr = A 
        k = 1
        while stack or curr :
            while curr :
                stack.append(curr)
                curr = curr.left 
            curr = stack.pop()
                        
            if k == B :
                return curr.val 
            k += 1
            curr = curr.right 
        return curr.val 
            
''' time complexity : O(n)                
    space complexity : O(n)
'''
